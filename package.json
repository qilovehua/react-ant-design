{
  "name": "react-ant-design",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.8.3",
    "babel-plugin-import": "^1.1.1",
    "echarts": "^3.5.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "node-less": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build-css": "node-sass public/ -o public/ --output-style expanded",
    "watch-css": "npm run build-css && node-sass public/ -o public/ --watch --recursive --output-style expanded",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
